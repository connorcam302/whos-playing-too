{
    "useTabs": true,
	"tabWidth": 2,
	"printWidth": 120,
	"trailingComma": "all",
	"bracketSpacing": true,
	"arrowParens": "avoid",
	"endOfLine": "auto",
    "semi": false,
	"singleQuote": true,
	"plugins": ["prettier-plugin-svelte"],
    "arrowParens": "always",
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
